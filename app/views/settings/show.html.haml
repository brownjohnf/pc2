.row
  .span3
    = render 'shared/menu'
  .span7
    :erb
      <p>
        <b>Property:</b>
        <%= @setting.property %>
      </p>

      <p>
        <b>Value:</b>
        <%= @setting.value %>
      </p>

      <p>
        <b>Explanation:</b>
        <%= @setting.explanation %>
      </p>

      <p>
        <b>Comment:</b>
        <%= @setting.comment %>
      </p>

      <p>
        <b>User:</b>
        <%= @setting.user_id %>
      </p>
  .span2
