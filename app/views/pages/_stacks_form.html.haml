- stack = @page.stacks.build
= bootstrap_form_for stack do |f|
  = f.hidden_field :stackable_id
  = f.hidden_field :stackable_type
  = f.collection_select :library_id, Library.order('name ASC').available(@page, (can?(:manage, Document) ? nil :current_user)), :id, :name
  = f.submit 'Add to library'
