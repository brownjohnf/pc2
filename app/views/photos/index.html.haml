.container
  .row
    .span3
      = render 'shared/menu'
    .span9
      %h1
        Photo Gallery
        %small= pluralize(Photo.count, 'photo')
      - @photos.each do |photo|
        = link_to image_tag(photo.photo.url(:icon), :width => '10%', :style => 'float:left;'), photo
      .clearfloat
      .center= will_paginate
      = render 'shared/search'
