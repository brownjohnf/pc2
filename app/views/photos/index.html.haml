.container
  .row
    .span3
      = render 'shared/menu'
    .span9
      %h1
        Photo Gallery
        %span= pluralize(Photo.count, 'photo')
      %ul.thumbnails
        - @photos.each do |photo|
          .span1= link_to image_tag(photo.photo.url(:icon)), photo, :class => 'thumbnail'
      .center= will_paginate
      = render 'shared/search'